{
  "name": "ndom91-home-2020",
  "description": "ndom91 homepage 2020",
  "version": "2.0.0",
  "author": "Nico Domino <yo@ndo.dev>",
  "keywords": [
    "gatsby",
    "typescript",
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "dev": "gatsby develop",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "format": "prettier 'src/**/*.{ts,tsx,css,md}'",
    "format:fix": "prettier --write 'src/**/*.{ts,tsx,css,md}'"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.7",
    "@types/typography": "^0.16.3",
    "gatsby": "^2.32.3",
    "gatsby-plugin-catch-links": "^2.10.0",
    "gatsby-plugin-lodash": "^3.10.0",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-offline": "^3.10.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sitemap": "^2.12.0",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-plugin-typescript": "^2.12.0",
    "gatsby-remark-autolink-headers": "^2.11.0",
    "gatsby-remark-copy-linked-files": "^2.10.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-prismjs": "^3.13.0",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-remark": "^2.16.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.20",
    "polished": "^4.1.0",
    "prettier": "^2.2.1",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "framer-motion": "^3.3.0",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-remark-images": "^3.11.0",
    "gatsby-remark-images-medium-zoom": "^1.7.0",
    "gatsby-remark-images-zoom": "^1.1.0",
    "gatsby-transformer-json": "^2.11.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "react-parallax-tilt": "^1.4.81",
    "react-use": "^17.1.0"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "printWidth": 140,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,css,json,md}": [
      "npm run format:fix",
      "git add"
    ]
  }
}
